# Creating a list of integers
numbers = [1, 2, 3, 4, 5]

# Accessing elements in the list using indexing
print(numbers[0])  # Output: 1
print(numbers[2])  # Output: 3

# Modifying elements in the list using indexing
numbers[1] = 10
print(numbers)  # Output: [1, 10, 3, 4, 5]

# Adding elements to the list using append() method
numbers.append(6)
print(numbers)  # Output: [1, 10, 3, 4, 5, 6]

# Removing elements from the list using remove() method
numbers.remove(3)
print(numbers)  # Output: [1, 10, 4, 5, 6]

# Finding the length of the list using len() function
print(len(numbers))  # Output: 5

# Looping through the list using a for loop
for number in numbers:
    print(number)

# List slicing to get a subset of elements
subset = numbers[1:4]
print(subset)  # Output: [10, 4, 5]

# Checking if an element exists in the list using 'in' keyword
print(10 in numbers)  # Output: True
print(7 in numbers)   # Output: False
